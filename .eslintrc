{
  "ecmaFeatures": {
    "jsx": true,
     "globalReturn": true,
    "modules": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
"react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/react-in-jsx-scope": 1,
    "no-labels": 0,
    "comma-spacing": 0,
    "no-undef": 0,
    "quotes": 0,
    "semi": 0,
    "camelcase": 0,
    "space-infix-ops": 0,
    "eqeqeq": 0,
    "padded-blocks": 0,
    "block-spacing": 0,
    "space-before-function-paren": 0,
    "keyword-spacing": 0,
    "space-before-blocks": 0,
    "key-spacing": 0,
    "arrow-spacing": 0,
    "no-trailing-spaces": 0,
    "no-mixed-spaces-and-tabs":0,
    "no-multi-spaces":0,
    "arrow-parens": 0,
    "indent": 0,
    "no-multiple-empty-lines": 0,
    "strict": 0
  },
  "extends": "standard",
  "parser": "babel-eslint"
}
